// this test case demonstrates the use of multiple arguments with a single output of type Number
// the threshold used here is 0.65
// 1319716423 ---> 0.5454545454545454 Hit Ratio
// Total methods instrumented: 1
// Total methods that can benefit from memoization: 0
// Percentage of candidates memoizable:0


function dotProduct(row, col) {
		var dotp = 0;
		// ensure col and row of M1 & M2 resp., are same
		if (row.length != col.length) {
			throw "invalid arguments";
		}
		for (var i = 0; i < row.length; i++) {
			dotp += row[i] * col[i];
		}
		return dotp;
}

dotProduct([5,3,7], [8,4,3]);
dotProduct([5,3,7], [8,4,3]);
dotProduct([5,3,7], [8,4,3]);
dotProduct([1,3,4], [2,4,6]);
dotProduct([5,3,7], [3,4,2]);
dotProduct([5,3,7], [8,4,3]);
dotProduct([5,3,7], [8,4,3]);
dotProduct([5,3,7], [8,4,3]);
dotProduct([5,3,7], [2,3,3]);
dotProduct([4,6,1], [8,4,3]);
dotProduct([5,3,7], [8,4,3]);
